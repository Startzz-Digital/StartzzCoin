<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="StartzzCoin" location="startMenu"/>
	<shortcut
		name="StartzzCoin Server"
		target="$INSTALL_PATH\StartzzCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start StartzzCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="StartzzCoin Wallet"
		target="http://localhost:36876/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch StartzzCoin wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>